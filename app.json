{
    "expo": {
        "name": "uber-clone",
        "slug": "uber-clone",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.speeddocs",
            "config": {
                "googleMapsApiKey": "AIzaSyBON2l1pLTHnchT_TB03BqwbpZWYvLRg3c"
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            },

            "package": "com.speeddocs",
            "config": {
                "googleMaps": {
                    "apiKey": "AIzaSyBON2l1pLTHnchT_TB03BqwbpZWYvLRg3c"
                },

                "permissions": [
                    "ACCESS_COARSE_LOCATION",
                    "ACCESS_FINE_LOCATION",
                    "FOREGROUND_SERVICE"
                ]
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
